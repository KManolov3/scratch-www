{
  "name": "com.advanceautoparts.instoreapps",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky install ./app/.husky && cd ./app",

    "start": "react-native start",

    "run:cycle-count": "react-native run-android --main-activity .activities.CycleCountActivity",
    "run:batch-count": "react-native run-android --main-activity .activities.BatchCountActivity",
    "run:outage": "react-native run-android --main-activity .activities.OutageActivity",
    "run:item-lookup": "react-native run-android --main-activity .activities.ItemLookupActivity",
    "run:truck-scan": "react-native run-android --main-activity .activities.TruckDetailScanActivity",

    "adb:reverse": "adb reverse tcp:8081 tcp:8081; adb reverse tcp:1337 tcp:1337; adb reverse tcp:8097 tcp:8097",

    "check": "npm run eslint && npm run tsc && npm run prettier:check",
    "tsc": "tsc -p ./ --noEmit",
    "eslint": "eslint . --max-warnings=0 --cache --ext .ts,.tsx,.js,.jsx",
    "prettier:check": "prettier -c .",
    "prettier:fix": "prettier -w .",

    "test": "jest",

    "graphql:codegen": "graphql-codegen",
    "graphql:watch": "graphql-codegen -w",

    "react-native:codegen": "cd android && ./gradlew generateCodegenArtifactsFromSchema",

    "package:clean": "rm android/app/src/main/assets/index.android.bundle",

    "package:uninstall:local": "adb uninstall com.advanceautoparts.instoreapps",
    "package:install:local": "adb install -r builds/in-store-apps-local.apk",
    "package:build:local": "(cd android && ENVFILE=.env ./gradlew assembleRelease) && mkdir -p builds && cp android/app/build/outputs/apk/release/app-release.apk builds/in-store-apps-local.apk",

    "package:uninstall:dev": "adb uninstall com.advanceautoparts.instoreapps.dev",
    "package:install:dev": "adb install -r builds/in-store-apps-dev.apk",
    "package:build:dev": "(cd android && ENVFILE=.env.dev ./gradlew assembleRelease) && mkdir -p builds && cp android/app/build/outputs/apk/release/app-release.apk builds/in-store-apps-dev.apk",

    "package:uninstall:qa": "adb uninstall com.advanceautoparts.instoreapps.qa",
    "package:install:qa": "adb install -r builds/in-store-apps-qa.apk",
    "package:build:qa": "(cd android && ENVFILE=.env.qa ./gradlew assembleRelease) && mkdir -p builds && cp android/app/build/outputs/apk/release/app-release.apk builds/in-store-apps-qa.apk",

    "package:uninstall:staging": "adb uninstall com.advanceautoparts.instoreapps.staging",
    "package:install:staging": "adb install -r builds/in-store-apps-staging.apk",
    "package:build:staging": "(cd android && ENVFILE=.env.staging ./gradlew assembleRelease) && mkdir -p builds && cp android/app/build/outputs/apk/release/app-release.apk builds/in-store-apps-staging.apk",

    "package:uninstall:production": "adb uninstall com.advanceautoparts.instoreapps",
    "package:install:production": "adb install -r builds/in-store-apps-production.apk",
    "package:build:production": "(cd android && ENVFILE=.env.production ./gradlew assembleRelease) && mkdir -p builds && cp android/app/build/outputs/apk/release/app-release.apk builds/in-store-apps-production.apk",

    "send-intent": "cd scripts && ./send-intent.sh 2> /dev/null"
  },
  "dependencies": {
    "@apollo/client": "^3.7.11",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/native": "^6.1.6",
    "eventemitter3": "^5.0.1",
    "graphql": "^16.6.0",
    "launchdarkly-react-native-client-sdk": "^7.1.4",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0",
    "newrelic-react-native-agent": "^1.2.1",
    "react-native-config": "^1.5.0",
    "react-native-device-info": "^10.6.0",
    "react-native-get-random-values": "^1.9.0",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-sound": "^0.11.2",
    "react-native-svg": "^13.9.0",
    "react-native-toast-message": "^2.1.6",
    "react-native": "0.71.6",
    "react": "18.2.0",
    "rtn-in-store-apps": "file:./react-native-modules/RTNInStoreApps",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/client-preset": "^3.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.28",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/lodash-es": "^4.17.7",
    "@types/luxon": "^3.3.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.1",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "~19.0.4",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.8.8",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
